# Generated by config_brpc.sh, don't modify manually
SYSTEM=Linux
HDRS=/usr/include/
LIBS=/usr/lib/x86_64-linux-gnu
PROTOC=/usr/bin/protoc
PROTOBUF_HDR=/usr/include/
CC=gcc
CXX=g++
GCC_VERSION=110400
STATIC_LINKINGS= -lgflags -lprotobuf -lleveldb -lsnappy
DYNAMIC_LINKINGS=-lpthread -lssl -lcrypto -ldl -lz -lrt
CPPFLAGS=-DBRPC_WITH_GLOG=0 -DGFLAGS_NS=google -D__const__=__unused__ -g
# without the flag, linux+arm64 may crash due to folding on TLS.
ifeq ($(CC),gcc)
  ifeq ($(shell uname -p),aarch64) 
    CPPFLAGS+=-fno-gcse
  endif
endif

ifeq ($(NEED_LIBPROTOC), 1)
    STATIC_LINKINGS+=-lprotoc
endif
ifeq ($(NEED_GPERFTOOLS), 1)
    $(error "Fail to find gperftools")
endif
ifeq ($(NEED_GTEST), 1)
    $(error "Fail to find gtest")
endif
